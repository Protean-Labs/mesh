    "@opam/piaf": "0.1.0",
    "@opam/ppx_string": "v0.14.1",
    "@opam/reason": "3.7.0",
    "@opam/rresult": "0.6.0",
    "@opam/yojson": "1.7.0",
    "ocaml": "4.12.0"
  },
  "devDependencies": {
    "@opam/dune": "2.9.0",
    "@opam/ocaml-lsp-server": "1.7.0",
